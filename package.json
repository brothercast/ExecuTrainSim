{
    "name": "executrain",
    "version": "1.0.0",
    "scripts": {
      "start": "concurrently \"npm run start-react\" \"npm run start-server\"",
      "start-react": "cd executrainsim && npm start",
      "start-server": "cd execuTrainServer && npm start",
      "build": "cd executrainsim && npm run build",
      "install-all": "npm install && cd executrainsim && npm install && cd ../execuTrainServer && npm install"
    },
    "dependencies": {
      "@azure/msal-node": "^2.15.0",
      "axios": "^1.7.7",
      "concurrently": "^9.0.1",
      "lucide-react": "^0.454.0",
      "react-dom": "^18.3.1",
      "react-scripts": "^5.0.1",
      "web-vitals": "^4.2.4"
    },
    "devDependencies": {
      "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
    }
  }